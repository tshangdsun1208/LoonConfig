US = url-test,US_Filter,url = http://cp.cloudflare.com/generate_204,interval = 600,img-url = https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/United_States.png
Load Balance = load-balance,ALL_Filter,url = http://cp.cloudflare.com/generate_204,interval = 600,algorithm = random,img-url = https://github.com/Koolson/Qure/blob/master/IconSet/Color/Blackhole.png?raw=true
ChatGPT Auto = url-test,ChatGPT-Filter,url = http://cp.cloudflare.com/generate_204,interval = 600,img-url = https://github.com/Koolson/Qure/blob/master/IconSet/Color/ChatGPT.png?raw=true
Microsoft = select,Available,Auto,Select,HK,TW,JP,SG,KR,US,url = http://cp.cloudflare.com/generate_204,img-url = https://github.com/blackmatrix7/ios_rule_script/blob/master/icon/color/microsoft.png?raw=true
YouTube = select,Available,Auto,Select,HK,TW,SG,KR,JP,US,url = http://cp.cloudflare.com/generate_204,img-url = https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/YouTube.png
Telegram = select,Available,Select,Auto,HK,TW,SG,JP,KR,US,url = http://cp.cloudflare.com/generate_204,img-url = https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Telegram.png
Apple = select,DIRECT,Available,Auto,Select,HK,SG,TW,JP,KR,US,url = http://cp.cloudflare.com/generate_204,img-url = https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Apple.png
Netflix = select,Netflix-Filter,url = http://cp.cloudflare.com/generate_204,img-url = https://raw.githubusercontent.com/Orz-3/mini/master/Color/Netflix.png
OpenAI = select,ChatGPT Auto,ChatGPT-Filter,url = http://cp.cloudflare.com/generate_204,img-url = https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Bot.png
Final = select,DIRECT,REJECT,Available,Select,HK,TW,SG,JP,KR,US,Auto,url = http://cp.cloudflare.com/generate_204,img-url = https://github.com/Koolson/Qure/blob/master/IconSet/Color/Final.png?raw=true

[Remote Filter]
ALL_Filter = NameRegex, FilterKey = ".*"
HK_Filter = NameRegex, FilterKey = "(?i)(港|HK|Hong)"
TW_Filter = NameRegex, FilterKey = "(?i)(台|TW|Tai)"
JP_Filter = NameRegex, FilterKey = "(?i)(日本|川日|东京|大阪|泉日|埼玉|沪日|深日|JP|Japan)"
KR_Filter = NameRegex, FilterKey = "(?i)(KR|Korea|KOR|首尔|韩|韓)"
US_Filter = NameRegex, FilterKey = "(?i)(美|波特兰|达拉斯|俄勒冈|凤凰城|费利蒙|硅谷|拉斯维加斯|洛杉矶|圣何塞|圣克拉拉|西雅图|芝加哥|US|United States)"
SG_Filter = NameRegex, FilterKey = "(?i)(新加坡|坡|狮城|SG|Singapore)"
ChatGPT-Filter = NameRegex, FilterKey = "(?i)ChatGPT"
Netflix-Filter = NameRegex, FilterKey = "(?i)(奈飞|奈|NF|Netflix)"

[Rule]
#Type:DOMAIN-SUFFIX,DOMAIN,DOMAIN-KEYWORD,USER-AGENT,URL-REGEX,IP-CIDR
#Strategy:DIRECT,PROXY,REJECT
#Options:no-resolve(only for IP-CIDR,IP-CIDR2,GEOIP,IP-ASN)

FINAL,Final

